  "scripts": {
    "start": "node dist/app.js",
    "build:server": "babel ./server/ -d ./dist",
    "postinstall": "npm run build:server",
    "migrations": "sequelize db:migrate",
    "server:dev":
      "NODE_ENV=development nodemon --watch server --exec babel-node  -- server/app.js ",
    "reset-db":
      "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate",
    "test":
      "npm run reset-db && NODE_ENV=test nyc --reporter=html --reporter=text mocha  --compilers js:babel-register ./server/tests/api/*.js -t 20000",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "start:dev": "npm run server:dev"
  },
